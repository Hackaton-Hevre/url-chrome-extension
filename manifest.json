{
  "manifest_version": 2,
  "name": "URLz",
  "version": "0.1",
  "permissions": ["activeTab","storage"],
  "browser_action": {
    "default_icon": "check.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {"16": "check.png", "32": "check32.png", "48": "check64.png", "128": "check128.png"}
}







